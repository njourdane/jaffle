{
  "name": "tidaml",
  "version": "0.1.0",
  "scripts": {
    "esbuild": "esbuild ./src/tidaml.js --bundle --outfile=./public/tidaml.js --target=es2021",
    "copy": "mkdir -p ./public && cp ./website/* ./public && cp -r tunes/ public/",
    "build": "npm run copy && npm run esbuild",
    "start": "npm run copy && npm run esbuild -- --servedir=./public"
  },
  "devDependencies": {
    "esbuild": "^0.18.4"
  },
  "dependencies": {
    "@codemirror/language": "^6.8.0",
    "@codemirror/legacy-modes": "^6.3.2",
    "@strudel.cycles/core": "^0.8.1",
    "@strudel.cycles/mini": "^0.8.1",
    "@strudel.cycles/soundfonts": "^0.8.1",
    "@strudel.cycles/tonal": "^0.8.1",
    "@strudel.cycles/webaudio": "^0.8.1",
    "@uiw/codemirror-theme-solarized": "^4.21.3",
    "codemirror": "^6.0.1",
    "js-yaml": "^4.1.0"
  }
}
